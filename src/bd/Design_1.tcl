  # Create ports
  set clk_board_125M [ create_bd_port -dir I clk_board_125M ]
  set resetn [ create_bd_port -dir I -type rst resetn ]
  set vid_data [ create_bd_port -dir O -from 23 -to 0 -type data vid_data ]
  set vid_hsync [ create_bd_port -dir O vid_hsync ]
  set vid_vsync [ create_bd_port -dir O vid_vsync ]

